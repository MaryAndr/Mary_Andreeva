<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="brightOrange">#FA6600</color>
    <color name="slycalendar_defBackgroundColor">#ffffff</color>
    <color name="slycalendar_defHeaderColor">#FA6600</color>
    <color name="slycalendar_defHeaderTextColor">#ffffff</color>
    <color name="slycalendar_defSelectedColor">#FA6600</color>
    <color name="slycalendar_defSelectedTextColor">#ffffff</color>
    <color name="slycalendar_defTextColor">#333333</color>
    <declare-styleable name="SlyCalendarView">
        <attr format="color" name="backgroundColor"/>
        <attr format="color" name="headerColor"/>
        <attr format="color" name="headerTextColor"/>
        <attr format="color" name="textColor"/>
        <attr format="color" name="selectedColor"/>
        <attr format="color" name="selectedTextColor"/>
        <attr format="boolean" name="firstMonday"/>
    </declare-styleable>
    <string name="app_name" translatable="false">SlyCalendarView</string>
    <string name="slycalendar_cancel">Cancel</string>
    <string name="slycalendar_dates_period" translatable="false">%1$s â€“ %2$s</string>
    <string name="slycalendar_next">Next</string>
    <string name="slycalendar_prev">Prev</string>
    <string name="slycalendar_save">Save</string>
    <string name="slycalendar_short" translatable="false">EE, dd</string>
    <string name="slycalendar_short_month" translatable="false">EE, dd MMM</string>
    <string name="slycalendar_singledate" translatable="false">EE, dd MMMM</string>
    <style name="SlyCalendarDialogStyle" parent="Base.Theme.AppCompat.Dialog">
        <item name="android:windowMinWidthMajor">100%</item>
        <item name="android:windowMinWidthMinor">100%</item>
    </style>
    <style name="SlyCalendarTimeDialogTheme" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorAccent">@color/brightOrange</item>
        <item name="colorControlNormal">@color/brightOrange</item>
    </style>
</resources>